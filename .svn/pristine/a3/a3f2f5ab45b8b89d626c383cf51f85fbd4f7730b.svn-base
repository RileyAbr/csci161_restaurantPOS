package Utilities;

import MenuItems.Item;
import java.util.Comparator;

/**
 *
 * @author riley.abrahamson
 */
public class PriorityComparator implements Comparator<Item> {

    /**
     * Returns whether the priority of one menu item is higher than the other
     * @param o1 the first menu item
     * @param o2 the second menu item
     * @return Negative if the first object has priority, 0 if the priority is equal, Positive if the second object has priority
     */
    @Override
    public int compare(Item o1, Item o2) 
    {
        return o1.getPriority() - o2.getPriority();
    }
    
}
